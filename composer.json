{
    "name": "ks/recruitment-php-code-test",
    "description": "PHP code test",
    "type": "library",
    "license": "MIT",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/apache/logging-log4php"
      }
    ],    
    "require": {
        "apache/log4php": "2.3.0",
        "topthink/think-log": "2.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Test\\": "tests/"
        },
        "classmap": [
            "src/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "process-timeout": 0
    }    
}
